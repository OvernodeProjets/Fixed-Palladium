[
  {
    "id": 1,
    "name": "Minecraft Java Edition | Paper",
    "description": "Minecraft Java Edition",
    "docker_image": "ghcr.io/pterodactyl/yolks:java_17",
    "startup": "java -Xms128M -XX:MaxRAMPercentage=85.0 --add-modules=jdk.incubator.vector -XX:+UseG1GC -XX:+ParallelRefProcEnabled -XX:+UnlockExperimentalVMOptions -XX:+AlwaysPreTouch -Dterminal.jline=false -Dterminal.ansi=true -jar {{SERVER_JARFILE}}",
    "settings": {
      "SERVER_JARFILE": "server.jar",
      "BUILD_NUMBER": "latest"
    },
    "limitsResources": {
      "max" : {
        "cpu": 1,
        "memory": 1024,
        "disk": 1024
      },
      "min": {
        "cpu": 1,
        "memory": 512,
        "disk": 512
      }
    }
  }
]